<div class="row my-5">
    {{#with service}}
    <div class="col-12 my-5">
        <div class="card text-bg-success text-center">
            <div class="card-body">
                <h2 class="card-title">{{title}}</h2>
                <p>{{description}}</p>
                {{#each buttons}}
                <a class="btn btn-primary" href="{{link}}">{{text}}</a>
                {{/each}}
            </div>
        </div>
    </div>
    {{/with}}
</div>